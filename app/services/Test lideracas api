
POST http://liderancas.net.br/api/leaders/login HTTP/1.1
content-type: application/json

{
    "cpf": "815.891.233-87"
}

###
GET http://liderancas.net.br/api/leaders/1 HTTP/1.1
content-type: application/json

###
GET http://liderancas.net.br/api/leaders/1/members HTTP/1.1
content-type: application/json

###
# Alterar Lider

POST http://liderancas.net.br/api/leaders/1 HTTP/1.1
Content-Type: application/json

{
"leader":{
    "address_attributes": {
        "description": "teste",
        "number": "12345",
        "complement": "",
        "zipcode": "64057-650",
        "district": "Vale Quem Tem",
        "street": "Rua Professora Maria Célia Nascimento",
        "city_id": 3186
        }
	}
}

###
# Método para buscar os membros pŕoximos não visitados

POST http://liderancas.net.br/api/members/find_unvisited HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.114644, 
 "longitude": -42.753744,
 "radius": 1000
}
###
# Método para buscar os membros pŕoximos, independente de status

POST http://liderancas.net.br/api/members/find HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.114644, 
 "longitude": -42.753744,
 "radius": 1000
}

###
# cadastro de visitas:

POST http://liderancas.net.br:3000/api/visits/create HTTP/1.1
content-type: application/json

{
	"visit":{
		"date_visit":"2019-10-27",
		"observation":"testeeee2",
		"number_of_voters": 4,
		"member_id": 1
	}
}

###
# Cadastro de membros:
POST http://liderancas.net.br/api/members/create HTTP/1.1
content-type: application/json

{
	"member":{
		"name": "membro teste da silva",
		"cpf": "111.111.111-11",
		"birthdate": "2019-10-27",
		"leader_id": 1,
		"address_attributes":{
			"description": "trabalho",
			"zipcode": "64057-650",
			"street": "retorno do cep",
			"number": 3365,
			"complement": "bloco apto lote",
			"district":"retorno do cep",
			"city_id": 3286
		}
		
	}
	
}

###
# Detalhes de um membro:
GET http://liderancas.net.br/api/members/1 HTTP/1.1