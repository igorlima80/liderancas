content-type: application/json

###

POST http://liderancas.net.br/api/leaders/login HTTP/1.1
content-type: application/json

{
    "cpf": "815.891.233-87"
}
###
GET http://liderancas.net.br/api/leaders/1/members HTTP/1.1
content-type: application/json

###
# Alterar Lider

POST http://liderancas.net.br/api/leaders/1 HTTP/1.1
content-type: application/json

{
    "id": 1,
    "mother_name": "maeeeeeiiii",
    "father_name": "paiiiuououoiii",
    "rg": "teste",
    "cpf": "815.891.233-87",
    "latitude": -25.4295963,
    "longitude": -49.2712724,
    "address": {
        "description": "",
        "number": "12345",
        "complement": "",
        "zipcode": "64057-650",
        "district": "Vale Quem Tem",
        "street": "Rua Professora Maria Célia Nascimento",
        "city_id": 3186
    }
}

###
# Método para buscar os membros pŕoximos não visitados

POST http://liderancas.net.br/api/members/find_unvisited HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.114644, 
 "longitude": -42.753744,
 "radius": 1000
}
###
# Método para buscar os membros pŕoximos, independente de status

POST http://liderancas.net.br/api/members/find HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.0814557, 
 "longitude": -42.7995088,
 "radius": 1000
}

###
# cadastro de visitas:

POST http://liderancas.net.br/api/visits/create HTTP/1.1
content-type: application/json

{
	"visit":{
		"date_visit":"2019-10-27",
		"observation":"testeeee2",
		"number_of_voters": 4,
		"member_id": 1
	}
}

###
# Cadastro de membros:
POST http://liderancas.net.br/api/members/create HTTP/1.1
content-type: application/json

{
    "name": "Teste 7",
    "latitude": -5.0707691,
    "longitude": -42.8156255,
    "cpf": "13245678912",
    "status": "unvisited",
    "birthdate": "1984-04-26",
    "translate_status": "Não Visitado",
    "leader": {
      "id": 1,
      "name": "Francisco Igor de Lima e Silva"
    },
    "address": {
      "description": "Casa",
      "number": "1129",
      "complement": "(Zona Norte) - de 801/802 a 1499/1500",
      "zipcode": "64002450",
      "district": "Marquês",
      "street": "Rua Magalhães Filho",
      "city_id": 3186
    }
  }
}

###
# Detalhes de um membro:
GET http://liderancas.net.br/api/members/1 HTTP/1.1
content-type: application/json

###
# Atualizar membro
PUT http://liderancas.net.br/api/members/1 HTTP/1.1
content-type: application/json

{
    "id": 1,
    "name": "membro teste da silva",
    "leader_id": 1,
    "cpf": "111.111.111-13",
    "status": "unvisited",
    "birthdate": "2019-10-27",
    "translate_status": "Não Visitado",
    "address": {
        "description": "casa",
        "number": "3365",
        "complement": "bloco apto lote",
        "zipcode": "64057-650",
        "district": "retorno do cep",
        "street": "retorno do cep",
        "city_id": 3286
    }
}

###
# Visita por membros

POST http://liderancas.net.br/api/visits/find_by_member HTTP/1.1
content-type: application/json

{
	"id": 1
}