GET http://liderancas.net.br/api/leaders/1 HTTP/1.1
content-type: application/json

###

POST http://liderancas.net.br/api/leaders/login HTTP/1.1
content-type: application/json

{
    "cpf": "815.891.233-87"
}
###
GET http://liderancas.net.br/api/leaders/1/members HTTP/1.1
content-type: application/json

###
# Alterar Lider

PUT http://liderancas.net.br/api/leaders/1 HTTP/1.1
content-type: application/json

{
    "mother_name": "maaaaae",
    "father_name": "pai",
    "rg": "123",
    "address": {
        "description": "Casa",
        "number": "",
        "complement": "",
        "zipcode": "",
        "district": "",
        "street": "",
        "city":{
          "id": "",
          "name_with_state": ""
        } 
    }
}

###
# Método para buscar os membros pŕoximos não visitados

POST http://liderancas.net.br/api/members/find_unvisited HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.0792706, 
 "longitude": -42.8137769,
 "radius": 3
}
###
# Método para buscar os membros pŕoximos, independente de status

POST http://liderancas.net.br/api/members/find HTTP/1.1
content-type: application/json

{
 "id":1,	
 "latitude": -5.0814557, 
 "longitude": -42.7995088,
 "radius": 3
}

###
# cadastro de visitas:

POST http://liderancas.net.br/api/visits HTTP/1.1
content-type: application/json

{
	"visit":{
		"date_visit":"2019-10-27",
		"observation":"testeeee2",
		"number_of_voters": 4,
		"member_id": 6
	}
}

###
# Cadastro de membros:
POST http://liderancas.net.br/api/members HTTP/1.1
content-type: application/json

{
    "name": "mauricio",
    "cpf": "00771173325",
    "birthdate": "2019-10-27",
    "address": {
        "description": "",
        "number": "",
        "complement": "",
        "zipcode": "64057-650",
        "district": "",
        "street": "",
        "city":{
          "id": "",
          "name_with_state": ""
        } 
    }
}

###
# Detalhes de um membro:
GET http://liderancas.net.br/api/members/4 HTTP/1.1
content-type: application/json

###
# Atualizar membro
PUT http://liderancas.net.br/api/members/6 HTTP/1.1
content-type: application/json

{
    "name": "jose",
    "cpf": "00771173325",
    "birthdate": "2019-10-27",
    "address": {
        "description": "",
        "number": "",
        "complement": "",
        "zipcode": "64057-650",
        "district": "",
        "street": "",
        "city":{
          "id": "",
          "name_with_state": ""
        } 
    }
}

###
# Visita por membros

POST http://liderancas.net.br/api/visits/find_by_member HTTP/1.1
content-type: application/json

{
	"id": 2
}

###

POST http://liderancas.net.br/api/cities/find HTTP/1.1
content-type: application/json

{
	"description":"teres"
}

###

POST http://liderancas.net.br/api/utils/zipcode HTTP/1.1
content-type: application/json

{
	"zipcode":"64057-650"
}

###

POST http://liderancas.net.br/api/cities/find_by_ibge HTTP/1.1
content-type: application/json

{
	"ibge": "2211001"
}